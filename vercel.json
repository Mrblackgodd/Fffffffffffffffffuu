{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "methods": ["POST"],
      "dest": "./index.js"
    }
  ],
  "env": {
    "TELEGRAM_BOT_TOKEN": "<YOUR_TELEGRAM_BOT_TOKEN>",
    "TELEGRAM_TARGET_CHAT_ID": "<YOUR_TARGET_CHAT_ID>"
  }
}
